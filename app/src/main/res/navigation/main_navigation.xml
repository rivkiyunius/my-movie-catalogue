<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_navigation"
    app:startDestination="@id/included_movie">

    <include-dynamic
        android:id="@+id/included_favorite"
        app:moduleName="favorite"
        app:graphResName="favorite_navigation"/>

    <include-dynamic
        android:id="@+id/included_search_movie"
        app:moduleName="search"
        app:graphResName="search_movie_navigation"/>

    <include-dynamic
        android:id="@+id/included_movie"
        app:moduleName="movie"
        app:graphResName="movie_navigation"/>

</navigation>